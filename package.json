	"dependencies": {
		"skrollr": "^0.6.26",
		"skrollr-menu": "^1.0.3",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.4",
		"concurrently": "^7.1.0",
		"connect": "^3.7.0",
		"eslint": "^8.13.0",
		"gulp": "^4.0.2",
		"gulp-atomizer": "^3.0.7",
		"gulp-concat": "^2.6.1",
		"gulp-file-include": "^2.3.0",
		"gulp-htmlmin": "^5.0.1",
		"gulp-inline": "^0.1.3",
		"gulp-newer": "^1.4.0",
		"gulp-postcss": "^9.0.1",
		"gulp-replace": "^1.1.3",
		"gulp-sourcemaps": "^3.0.0",
		"htmlhint": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"opn": "^6.0.0",
		"postcss-assets": "^6.0.0",
		"precss": "^4.0.0",
		"serve-static": "^1.15.0",
		"stylelint": "^14.6.1",
		"stylelint-config-standard": "^25.0.0"
	},
	"browserslist": [
		"safari 9",
		"ie 11"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "eslint --fix",
		"*.htm": "htmlhint",
		"*.scss": "stylelint --fix"
	}
}
